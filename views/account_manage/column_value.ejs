<% _.each(data, function (obj) { %>
  <tr>
    <td><%= obj.id %></td>
    <td><%= obj.name %></td>
    <td><%= obj.email %></td>

    <% if (obj.role == undefined) {%>
      <td></td>
    <%} else {%>
      <td><%= obj.role.name %></td>
    <%} %>

    <td><%= obj.mobile %></td>      
    
    <% if (obj.deleted == true) { %>
      <td>Deleted</td>
    <% } else { %>
      <td>Activated</td>
    <% } %> 

    <td>
      <button type="button" id="btnUpdate<%= obj.id %>" onClick="update(<%= JSON.stringify(obj) %>)" class="btn btn-primary update">Update</button> 
      <% if (obj.deleted == false) { %>
        <button type="button" id="btnRemove<%= obj.id %>" onClick="removeObject(<%= JSON.stringify(obj) %>)" class="btn btn-danger remove">Delete</button>
      <% }%>
    </td>                            
  </tr>
<% }) %>