<% _.each(data, function (obj) { %>
  <tr>

    <td><%= obj.billid %></td>
    <td><%= obj.customer.name %></td>
    <td><%= obj.tax %></td>
    <td><%= obj.discount %></td>
    <td><%= obj.coupon %></td>
    <td><%= obj.user.name %></td>
    <td><%= obj.time %></td>
    <td><%= obj.store.name %></td>
    <td><%= obj.subtotal %></td>
    <td><%= obj.total %></td>    
    <td><%= obj.received %></td>
    <td><%= obj.totalitem %></td>

    <td>
      <button type="button" id="btnDetail<%= obj.id %>" onClick="detail(<%= obj.id %>)" class="btn btn-primary update">Detail</button> 
    </td>

  </tr>
<% }) %>